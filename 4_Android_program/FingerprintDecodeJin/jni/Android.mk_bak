LOCAL_PATH := $(call my-dir)
LIB_PATH := $(LOCAL_PATH)/lib
INCLUDE_PATH := $(LOCAL_PATH)/include
FFMPEG_PATH = $(LOCAL_PATH)/ffmpeg-0-11-1
MAIN_PATH := $(LOCAL_PATH)/fingerprint

include $(CLEAR_VARS)
LOCAL_MODULE := avformat
LOCAL_SRC_FILES :=$(LIB_PATH)/libavformat.a
LOCAL_CFLAGS :=-Ilibavformat
LOCAL_EXPORT_C_INCLUDES := $(INCLUDE_PATH)/libavformat
LOCAL_EXPORT_CFLAGS := -Ilibavformat
LOCAL_EXPORT_LDLIBS := -llog
include $(PREBUILT_STATIC_LIBRARY)
include $(CLEAR_VARS)
LOCAL_MODULE := avcodec
LOCAL_SRC_FILES := $(LIB_PATH)/libavcodec.a
LOCAL_CFLAGS :=-Ilibavcodec
LOCAL_EXPORT_C_INCLUDES := $(INCLUDE_PATH)/libavcodec
LOCAL_EXPORT_CFLAGS := -Ilibavcodec
LOCAL_EXPORT_LDLIBS := -llog
include $(PREBUILT_STATIC_LIBRARY)
include $(CLEAR_VARS)
LOCAL_MODULE := avutil
LOCAL_SRC_FILES := $(LIB_PATH)/libavutil.a
LOCAL_CFLAGS :=-Ilibavutil
LOCAL_EXPORT_C_INCLUDES := $(INCLUDE_PATH)/libavutil
LOCAL_EXPORT_CFLAGS := -Ilibavutil
LOCAL_EXPORT_LDLIBS := -llog
include $(PREBUILT_STATIC_LIBRARY)
include $(CLEAR_VARS)
LOCAL_MODULE := swscale
LOCAL_SRC_FILES := $(LIB_PATH)/libswscale.a
LOCAL_CFLAGS :=-Ilibavutil -Ilibswscale
LOCAL_EXPORT_C_INCLUDES := $(INCLUDE_PATH)/libswscale
LOCAL_EXPORT_CFLAGS := -Ilibswscale
LOCAL_EXPORT_LDLIBS := -llog -lavutil
include $(PREBUILT_STATIC_LIBRARY)

include $(CLEAR_VARS)
LOCAL_MODULE    := FingerprintJni          #注意该名称跟上面的一致
LOCAL_C_INCLUDES += $(LOCAL_PATH) \
    $(LOCAL_PATH)/include
LOCAL_SRC_FILES := FingerprintAndroid.c
LOCAL_LDLIBS    := -L$(LOCAL_PATH) -lm -lz
LOCAL_STATIC_LIBRARIES := avformat avcodec avutil swscale
include $(BUILD_SHARED_LIBRARY)