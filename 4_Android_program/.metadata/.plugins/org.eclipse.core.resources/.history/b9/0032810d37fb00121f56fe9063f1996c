#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <jni.h>

#include <libavcodec/avcodec.h>
#include <libavformat/avformat.h>
#include <libavutil/avutil.h>
#include <libswscale/swscale.h>


/*
 * Class:     cc_omusic_Fingerprint_decoder_jni_FingerprintWraper
 * Method:    getVersionOfDecoder
 * Signature: (V)Ljava/lang/String;
 */
JNIEXPORT jstring JNICALL Java_cc_omusic_Fingerprint_1decoder_1jni_FingerprintWraper_getVersionOfDecoder
  (JNIEnv *env, jobject thiz)
JNIEXPORT jstring JNICALL Java_cc_omusic_Fingerprint_1decoder_1jni_FingerprintWraper_getVersionOfDecoder
(JNIEnv *env, jobject thiz)
{

	//printf(" this ffmpeg version: %d \n", avcodec_version() );

	char str[25];
	sprintf(str, "%d", avcodec_version());
	return (*env)->NewStringUTF(env,str);
}
